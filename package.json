{
  "name": "deltatest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/DeltaTest/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",
    "clean": "cd android && gradlew clean && cd ..",
    "android-build-debug": "cd android/ && gradlew assembleDebug",
    "android-release": "cd android && gradlew assembleRelease --no-daemon",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file ./index.js --bundle-output ./android/app/src/main/assets/index.bundle --sourcemap-output ./android/app/src/main/assets/index.map --assets-dest ./android/app/src/main/res/",
    "restartADB": "cd android && adb kill-server && adb start-server && cd ..",
    "adb": "cd android && adb reverse tcp:9090 tcp:9090 && cd ..",
    "adbDevTool": "cd android && adb reverse tcp:8097 tcp:8097 && cd ..",
    "debug": "npx react-devtools",
    "start": "npx react-native start",
    "test": "jest",
    "jetify": "npx jetify",
    "reset": "npx react-native start --reset-cache",
    "prettier": "prettier --write \"src/**/*.{js,ts,tsx}*\" \"*.js\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/netinfo": "^7.1.7",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-appstate-hook": "^1.0.6",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-get-random-values": "^1.7.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-vector-icons": "^9.0.0",
    "react-query": "^3.34.6",
    "react-redux": "^7.2.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/": "react-navigation/native",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/react-native": "^0.66.4",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-query": "^1.2.9",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.18",
    "@types/styled-components-react-native": "^5.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^7.14.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "jest": "^26.6.3",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "reactotron-react-native": "^5.0.1",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17",
    "styled-components": "^5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
